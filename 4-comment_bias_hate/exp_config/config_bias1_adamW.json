    "max_seq_len": 128,
    "train_epochs": 8,
    "adam_epsilon": 1e-8,
    "seed": 42,
    "train_batch_size": 64,
    "eval_batch_size": 128,
    "learning_rate": 5e-5,
    "warmup_proportion": 0,
    "run": "bias1_adamW",
    "patience": 10
}
