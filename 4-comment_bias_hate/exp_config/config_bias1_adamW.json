{
    "data_dir": "./data",
    "result_dir": "./result/",
    "config_dir": "./exp_config/",
    "pretrained_model": "beomi/KcELECTRA-base",  
    "architecture": "ElectraForSequenceClassification",
    "tokenizer_class": "BertTokenizer",
